#place in .git/config
#注意自己的remote會不一樣,像此處的10~12行一樣
#修改後放入 .git/config，再下了git checkout -b kero kero/master 之後
#以後每次可以下 git checkout kero; git pull 這種指令，將kero的master branch拉到本地端kero branch
#下 git checkout master;git merge kero 及可以將branch kero merge到master
[core]
	repositoryformatversion = 0
	filemode = true
	bare = false
	logallrefupdates = true
[i18n]
	commitencoding = utf-8
[remote "origin"]
	url = git@github.com:jcppkkk/ICS.git
	fetch = +refs/heads/*:refs/remotes/origin/*
[remote "kero"]
	url = git://github.com/kero033/ICS.git
	fetch = +refs/heads/*:refs/remotes/kero/*
[remote "hung"]
	url = git://github.com/hkegbert/ICS.git
	fetch = +refs/heads/*:refs/remotes/hung/*
[remote "weeseng"]
	url = git://github.com/weeseng/ICS.git
	fetch = +refs/heads/*:refs/remotes/weeseng/*
[remote "moon"]
	url = git://github.com/Moonloki/ICS.git
	fetch = +refs/heads/*:refs/remotes/moon/*
[branch]
	autosetupmerge = true
[branch "moon"]
	remote = moon
	merge = refs/heads/master
[branch "master"]
	remote = origin
	merge = refs/heads/master
[branch "weeseng"]
	remote = weeseng
	merge = refs/heads/master
[branch "kero"]
	remote = kero
	merge = refs/heads/master
[branch "hung"]
	remote = hung
	merge = refs/heads/master
